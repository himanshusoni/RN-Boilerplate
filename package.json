{
  "name": "RN_Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint app",
    "pretty": "prettier --config ./.prettierrc --write \"app/**/*.js\"",
    "pretty-lint": "prettier-eslint --eslint-config-path .eslintrc --write \"app/**/*.js\"",
    "flow": "flow",
    "flow-stop": "flow stop",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-dom": "^16.4.1",
    "react-native": "0.55.4",
    "react-native-navigation": "^2.0.2358",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.12",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/react-native": "^3.4.7",
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.67.0",
    "jest": "23.1.0",
    "prettier": "^1.13.5",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/app/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
